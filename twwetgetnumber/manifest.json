{
      "name": "Tweeter Filter",
      "version": "1.0",
      "manifest_version": 2,
	  "background": {
		"persistent": false,
		"scripts": ["js/background.js"]
      },
	  "permissions": [
		"<all_urls>",
		"tabs"
	  ],
      "content_scripts": [
        {
          "matches": ["*://*.twitter.com/search*" ],
          "js": ["js/jquery-3.2.1.min.js", "js/content.js"]
        }
      ],
	  "browser_action": {
          "default_title": "Fiter"
	   }
	}